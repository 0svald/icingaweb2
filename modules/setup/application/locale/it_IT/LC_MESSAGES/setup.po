#: clicommands/ConfigCommand.php:39 clicommands/ConfigCommand.php:140
#: clicommands/TokenCommand.php:35 clicommands/TokenCommand.php:67
msgid ""
"The argument --config expects a path to Icinga Web 2's configuration files"
msgstr ""
"Specifica nell'argomento --config il percorso ai file di configurazione di "
"Icinga Web 2"

#: clicommands/ConfigCommand.php:46
msgid ""
"The argument --group expects a owner group for the configuration directory"
msgstr ""
"Specifica nell'argomento --group il gruppo proprietario della cartella di "
"configurazione"

#: clicommands/ConfigCommand.php:53
msgid ""
"The argument --mode expects an access mode for the configuration directory"
msgstr ""
"Specifica nell'argomento --mode la modalità di accesso alla cartella di "
"configurazione"

