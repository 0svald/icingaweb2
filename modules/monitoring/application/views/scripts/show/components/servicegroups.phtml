<?php

if (empty($object->servicegroups)) return;

$list = array();
foreach ($object->servicegroups as $name => $alias) {
    $list[] = $this->qlink($alias, 'monitoring/list/services', array(
        'servicegroups' => $name
    ));
}
echo '<b>Servicegroups:</b> ' .  implode(', ', $list) . "<br />\n";

